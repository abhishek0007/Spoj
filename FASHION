#include<iostream>
#include<algorithm>
using namespace std;
int a[1000],b[1000];
void program()
{
int n;
cin>>n;
int i,j;

for(i=0;i<n;i++)
cin>>a[i];
sort(a,a+n);

for(i=0;i<n;i++)
cin>>b[i];
sort(b,b+n);

int sum =0;
for(i=0;i<n;i++)
sum += a[i]*b[i];


cout<<sum<<endl;
}
int main()
{

int t;
cin>>t;

while(t--)
{

program();
}
return 0;

}
